{
  "expo": {
    "name": "FortRoupel",
    "slug": "FortRoupel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcons/android/mipmap-xxxhdpi/ic_launcher_round.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcons/android/mipmap-xxxhdpi/ic_launcher_round.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.roupel.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
